{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console" : false,
        "interface-name" : false,
        "max-classes-per-file" : false,
        "align" : false,
        "variable-name" : [true, "check-format", "allow-leading-underscore"],
        "interface-over-type-literal" : false,
        "max-line-length" : [true, 180]
    },
    "rulesDirectory": []
}